@model homework3.Models.Model
@using homework3.Models
 
@{
    ViewBag.Title = "Table";
}

<h2>@ViewBag.Title</h2>
@{
    var list = homework3.Models.Model.multiplication(Model.Num);
    int index = 0;
}
<table class="table">
    
     
    @for (int i = 0; i < Model.Num; i++)
    {
        
        <tr>
            @for (int j = 0; j < Model.Num; j++)
            {
                <td>
                    @list[index]
                </td>
                index++;
            }
        </tr>
    }
        

</table>




          
